{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@styles/*": ["src/styles/*"],
      "@pages/*": ["src/pages/*"],
      "@redux/*": ["src/redux/*"],
      "@assets/*": ["src/assets/*"],
      "@utils/*": ["src/utils/*"],
      "@hooks/*": ["src/hooks/*"],
      "@layout/*": ["src/layouts/*"],
      "@routes/*": ["src/routes/*"],
      "@data/*": ["src/data/*"],
      "@hook/*": ["src/hook/*"]
    }
  },
  "exclude": ["node_modules"]
}